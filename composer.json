{
    "name": "sleep-cat/utils",
    "description": "PHP util package",
    "keywords": [
        "php",
        "tool kit",
        "work box"
    ],
    "type": "library",
    "homepage": "https://github.com/sleep-cat/utils",
    "license": "MIT",
    "authors": [
        {
            "name": "weifeng",
            "email": "xcxxkj@aliyun.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.5",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "SleepCat\\Utils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
